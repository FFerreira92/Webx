    <!-- Custom css-->
    <link rel="stylesheet" type="text/css" href="~/css/site.css">

</head>

<body class="theme-color1 light ltr">

    <!-- header start -->
    <header id="home">
        <div class="top-header">
            <div class="container-fluid-lg">
                <div class="row">
                    <div class="col-auto d-xl-block d-none">
                        <ul class="border-list">
                            <li>WebX ecommerce always free delivery</li>                            
                        </ul>
                    </div>
                    <div class="col-auto d-sm-block d-none">
                       
                    </div>
                    <div class="col-auto">
                            @if(!this.User.Identity.IsAuthenticated){
                               <ul class="border-list">
                                    <li>
                                        <div class="dropdown top-header-dropdown">
                                            <a class="dropdown-toggle" href="javascript:void(0)" role="button"
                                                id="dropdownMenuLink" data-bs-toggle="dropdown">
                                                <span>Login & Register</span>
                                                <i class="fas fa-chevron-down"></i>
                                            </a>
                                            <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="dropdownMenuLink">
                                                <li class="w-100">
                                                    <a class="dropdown-item" asp-action="Login" asp-controller="Account">Log In</a>
                                                </li>
                                                <li class="w-100">
                                                    <a class="dropdown-item" asp-action="Register" asp-controller="Account">Register</a>
                                                </li>
                                            </ul>
                                        </div>
                                    </li>
                               </ul>
                            }
                            
                            @if(this.User.Identity.IsAuthenticated)
                            {
                              
                                <ul class="border-list">
                                 
                                  <li>
                                      <div class="top-header-dropdown">
                                          <a asp-action="ViewUser" asp-controller="Account" style="padding: 5px;"><i class="far fa-user-robot" style="font-size:20px;margin-right: 10px;"></i>@ViewBag.UserFullName</a>
                                      </div>
                                      
                                  </li>

                                  @if (!this.User.IsInRole("Customer") && ViewBag.IsActive == true)
                                  {
                                      <li>
                                          <div class="top-header-dropdown">
                                              <a asp-action="Index" asp-controller="AdminPanel" style="padding: 5px;"><i class="fas fa-tools" style="font-size:20px;margin-right: 10px;"></i> Admin Panel</a>
                                          </div>
                                      </li>
                                  }


                                  <li>
                                      <div class="top-header-dropdown">
                                          <a asp-action="Logout" asp-controller="Account"><i class="fal fa-sign-out" style="font-size:20px;margin-right: 10px;"></i> Logout</a>
                                      </div>
                                  </li>
                                </ul>                              
                            }                          
                    </div>
                </div>
            </div>
        </div>
        <div class="main-header navbar-searchbar">
            <div class="container-fluid-lg">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="main-menu">
                            <div class="menu-left">
                                <div class="brand-logo">
                                    <a asp-action="Index" asp-controller="Home">                                       
                                        <img src="~/assets/images/logo.png" class="img-fluid blur-up lazyload" alt="logo">
                                        <img src="~/assets/images/logoloop.gif" class="img-fluid blur-up lazyload" alt="logo">
                                    </a>
                                </div>
                                <div class="category-menu">
                                    <a type="button" asp-action="Index" asp-controller="Products" asp-route-categoryString="AllCategories"
                                        class="btn btn-solid-default toggle-category d-sm-block d-none" style="min-width:158px;height:51.4px;display:flex !important;align-items:center;justify-content:center">
                                        <i style="font-size:13px;padding-right:15px" class="fal fa-shopping-bag"></i>
                                        Shop                                       
                                    </a>                                                              
                                </div>
                            </div>
                        
                           
                            <!--Partial View do Carrinho-->
                            <div id="cartPartialDiv">
                                <partial id="cartPartial" name="_CartDropDownPartial" ></partial>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- header end -->

    <!-- mobile fix menu start -->
    <div class="mobile-menu d-sm-none">
        <ul>
            <li>
                <a asp-action="Index" asp-controller="Home" class="active">
                    <i data-feather="home"></i>
                    <span>Home</span>
                </a>
            </li>
            <li>
                <a href="javascript:void(0)" class="toggle-category">
                    <i data-feather="align-justify"></i>
                    <span>Category</span>
                </a>
            </li>
            <li>
                <a href="cart.html">
                    <i data-feather="shopping-bag"></i>
                    <span>Cart</span>
                </a>
            </li>           
            <li>
                <a asp-action="ViewUser" asp-controller="Account">
                    <i class="far fa-user-robot" style="font-size:24px;display:block"></i>
                    <span>Account</span>
                </a>
            </li>
        </ul>
    </div>
    <!-- mobile fix menu end -->

        <div id="container" class="container-fluid">
            @RenderBody()
        </div>

    <!-- brand section start -->

    <div id="brandsPartialDiv">
         <partial id="brandsPartial" name="_brandsPartial" ></partial>
    </div>

   

    <!-- footer start -->
    <footer class="footer-sm-space">
        <div class="main-footer" style="background-color: #f0f3f8;position: relative;padding:20px 0 !important">
            <div class="container">
                <div class="row gy-4" style="justify-content: center;display: flex;align-items: center;">
                   
                    <div class="col-xl-3 col-lg-4 col-md-6" style="display: flex;justify-content: center;align-items: center;">
                        <div class="footer-contact">
                            <div class="brand-logo">
                                <a asp-action="Index" asp-controller="Home" class="footer-logo" style="margin-bottom:0px !important">                                    
                                    <img src="~/assets/images/logofooter.png" class="img-fluid blur-up lazyload" alt="logo">
                                </a>
                            </div>                           
                        </div>
                    </div>                   
                    <div class="col-xl-3 col-lg-3 col-md-4 col-sm-6" style="text-align:center">
                        <div class="footer-links">
                            <div class="footer-title">
                                <a asp-action="Index" asp-controller="Products"> 
                                    <h3>Shop</h3>
                                </a>
                            </div>                            
                        </div>
                    </div>
                    <div class="col-xl-3 col-lg-3 col-md-4 col-sm-6" style="text-align:center">
                        <div class="footer-links">
                            <div class="footer-title">
                                <a asp-action="AboutUs" asp-controller="Home">
                                    <h3>About Us</h3>
                                </a>
                            </div>                            
                        </div>
                    </div>
                    <div class="col-xl-3 col-lg-4 col-sm-6 d-none d-sm-block" style="text-align:center">
                        <div class="footer-links">
                            <div class="footer-title">
                                <a asp-action="ContactUs" asp-controller="Home">
                                    <h3>Contact Us</h3>
                                </a>
                            </div>
                        </div>                        
                    </div>
                    
                </div>
            </div>
        </div>
        <div class="sub-footer">
            <div class="container">
                <div class="row gy-3">
                    <div class="col-md-6">
                        <ul>
                            <li class="font-dark">We accept:</li>                           
                            <li>
                                <a href="javascript:void(0)">
                                    <img src="~/assets/images/payment-icon/4.jpg" class="img-fluid blur-up lazyload"
                                        alt="payment icon"></a>
                            </li>
                        </ul>
                    </div>
                    <div class="col-md-6">
                        <p class="mb-0 font-dark">WebX © 2022 All Rights Reserved</p>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- footer end -->

    
      

    <!-- tap to top Section Start -->
    <div class="tap-to-top">
        <a href="#home">
            <i class="fas fa-chevron-up"></i>
        </a>
    </div>
    <!-- tap to top Section End -->

    <div class="bg-overlay"></div>

    <!-- latest jquery-->
    <script src="~/assets/js/jquery-3.5.1.min.js"></script>
    
    <!-- Bootstrap js-->
    <script src="~/assets/js/bootstrap/bootstrap.bundle.min.js"></script>

    <!-- feather icon js-->
    <script src="~/assets/js/feather/feather.min.js"></script>
    
    <!-- Custom JS -->
    <script src="~/js/site.js" asp-append-version="true"></script>  

    <!-- lazyload js-->
    <script src="~/assets/js/lazysizes.min.js"></script>

    <!-- Add To Home js -->
    @*<script src="~/assets/js/pwa.js"></script>*@

    <!-- Slick js-->
    <script src="~/assets/js/slick/slick.js"></script>
    <script src="~/assets/js/slick/slick-animation.min.js"></script>
    <script src="~/assets/js/slick/custom_slick.js"></script>


    <!-- newsletter js -->
    <script src="~/assets/js/newsletter.js"></script>

    <!-- Notify js-->
    <script src="~/assets/js/bootstrap/bootstrap-notify.min.js"></script>

    <!-- script js -->
    @*<script src="~/assets/js/theme-setting.js"></script>*@
    <script src="~/assets/js/script.js"></script>
    <script src="~/assets/js/home-script.js"></script>

    <!-- Syncfusion ASP.NET Core Script Manager -->
    
    @await RenderSectionAsync("Scripts", required: false)
    <ejs-scripts></ejs-scripts>
    @await Component.InvokeAsync("Notyf")



